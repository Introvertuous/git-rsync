    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "main": "dist/src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Introvertuous/git-rsync.git"
  },
  "author": "Chris Ryan",
  "bugs": {
    "url": "https://github.com/Introvertuous/git-rsync/issues"
  },
  "homepage": "https://github.com/Introvertuous/git-rsync#README",
  "devDependencies": {
    "@types/bonjour": "^3.5.5",
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^1.7.5",
    "@types/find-up": "^2.1.1",
    "@types/inquirer": "0.0.43",
    "@types/ip": "^0.0.32",
    "@types/node": "^10.12.9",
    "@types/nodegit": "^0.22.5",
    "@types/shortid": "0.0.29",
    "@types/tmp": "^0.0.33",
    "@types/ws": "^6.0.1",
    "introvertuous-configurations": "^1.3.2",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6",
    "travis-deploy-once": "^5.0.9",
    "semantic-release": "^15.12.1"
  },
