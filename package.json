    "app-module-path": "^2.2.0",
    "bonjour": "^3.5.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "commander": "^2.19.0",
    "find-up": "^3.0.0",
    "inquirer": "^6.2.0",
    "ip": "^1.1.5",
    "nodegit": "^0.23.0",
